{
  "name": "@itznotabug/node-amp",
  "homepage": "https://github.com/itznotabug/node-amp",
  "description": "Refactored Appwrite Node SDK with support for named parameter objects",
  "version": "1.5.2",
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "type": "commonjs",
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true
  },
  "scripts": {
    "build": "tsup",
    "lint": "prettier . --check",
    "format": "prettier . --write"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./file": {
      "import": {
        "types": "./dist/inputFile.d.mts",
        "default": "./dist/inputFile.mjs"
      },
      "require": {
        "types": "./dist/inputFile.d.ts",
        "default": "./dist/inputFile.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/itznotabug/node-amp"
  },
  "devDependencies": {
    "@types/node": "20.11.25",
    "esbuild-plugin-file-path-extensions": "^2.0.0",
    "prettier": "^3.3.3",
    "tslib": "2.6.2",
    "tsup": "7.2.0",
    "typescript": "5.4.2"
  },
  "dependencies": {
    "node-fetch-native-with-agent": "1.7.2"
  }
}
